            "surah": 75,
            "nomor": 29,
            "ar": "\u0648\u064e\u0627\u0644\u0652\u062a\u064e\u0641\u0651\u064e\u062a\u0650 \u0627\u0644\u0633\u0651\u064e\u0627\u0642\u064f \u0628\u0650\u0627\u0644\u0633\u0651\u064e\u0627\u0642\u0650\u06d9",
