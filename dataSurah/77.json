        {
            "id": 5651,
            "surah": 77,
            "nomor": 29,
            "ar": "\u0627\u0650\u0646\u0652\u0637\u064e\u0644\u0650\u0642\u064f\u0648\u0652\u0653\u0627 \u0627\u0650\u0644\u0670\u0649 \u0645\u064e\u0627 \u0643\u064f\u0646\u0652\u062a\u064f\u0645\u0652 \u0628\u0650\u0647\u0656 \u062a\u064f\u0643\u064e\u0630\u0651\u0650\u0628\u064f\u0648\u0652\u0646\u064e\u06da",
