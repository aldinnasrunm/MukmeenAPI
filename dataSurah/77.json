        {
            "id": 5653,
            "surah": 77,
            "nomor": 31,
            "ar": "\u0644\u064e\u0627 \u0638\u064e\u0644\u0650\u064a\u0652\u0644\u064d \u0648\u0651\u064e\u0644\u064e\u0627 \u064a\u064f\u063a\u0652\u0646\u0650\u064a\u0652 \u0645\u0650\u0646\u064e \u0627\u0644\u0644\u0651\u064e\u0647\u064e\u0628\u0650\u06d7",
