        {
            "id": 5652,
            "surah": 77,
            "nomor": 30,
            "ar": "\u0627\u0650\u0646\u0652\u0637\u064e\u0644\u0650\u0642\u064f\u0648\u0652\u0653\u0627 \u0627\u0650\u0644\u0670\u0649 \u0638\u0650\u0644\u0651\u064d \u0630\u0650\u064a\u0652 \u062b\u064e\u0644\u0670\u062b\u0650 \u0634\u064f\u0639\u064e\u0628\u064d ",
